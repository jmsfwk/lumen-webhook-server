{
    "name": "jmsfwk/lumen-webhook-server",
    "description": "Send webhooks in Lumen apps",
    "keywords": [
        "lumen-webhook-server",
        "webhook",
        "server"
    ],
    "homepage": "https://github.com/jsmfwk/lumen-webhook-server",
    "license": "MIT",
    "authors": [
        {
            "name": "James Fenwick",
            "email": "j.fenwick@me.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "spatie/laravel-webhook-server": "^1.4"
    },
    "require-dev": {
        "laravel/lumen-framework": "^5.8|^6.0",
        "phpunit/phpunit": "^8.2",
        "spatie/test-time": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jmsfwk\\WebhookServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmsfwk\\WebhookServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
